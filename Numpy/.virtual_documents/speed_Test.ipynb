# using numpy arreys
import numpy as np
import time
size = 1_000_000_00
l1 = np.array(list(range(size)))
l2 = np.array(list(range(size)))
    
start = time.time()
add = l1 + l2

end = time.time()
print("timing of" ,add[0:10] ,'\n',end-start, )
    


# using python list

import time

size = 1_000_000_00
l1 = list(range(size))
l2 = list(range(size))
    
start = time.time()
add = [x+y for x, y in zip(l1,l2)]
    
end = time.time()
print("timing of" ,add[0:10],end-start)


